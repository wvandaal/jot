<header id="JOT-INFO">
  <h1><%= jot.escape('title') %></h1>
  <span class="byline">by <a href="#/users/<%= jot.get('user_id') %>"><%= jot.escape('author') %></a></span>
  <h2><%= jot.escape('description') %></h2>
</header>
<div id="JOT-WINDOW">
  <%= marked(jot.get('content')) %>
</div>
<header id="COMMENTS-HEADER">
  <h3>Comments</h3>
</header>
<div id="JOT-COMMENTS">
  
</div>